{
    "manifest_version": 3,
    "name": "HTML Reader",
    "version": "1.0",
    "description": "Reads the HTML of the current page",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": [
        "http://youtube/*",
        "https://youtube/*"
      ],
    "action": {
      "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://youtube/*",
                "https://youtube/*",
                "http://www.youtube.com/*",
                "https://www.youtube.com/*"
            ],
            "js": ["content_scripts/content.js"]
        }
    ]
  }
  